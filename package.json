{
  "name": "reason-logger",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "ReasonLoggerApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["reason-logger.lib"],
      "main": "TestReasonLogger",
      "name": "TestReasonLogger.exe"
    },
    "library": {
      "name": "reason-logger.lib",
      "namespace": "ReasonLogger"
    },
    "executable": {
      "require": ["reason-logger.lib"],
      "main": "ReasonLoggerApp",
      "name": "ReasonLoggerApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestReasonLogger.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
